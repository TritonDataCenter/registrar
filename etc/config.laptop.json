{
    "registration": {
        "domain": "test.laptop.joyent.us",
        "type": "host",
        "aliases": [
            "alias.laptop.joyent.us"
        ]
    },
    "zookeeper": {
        "connectTimeout": 1000,
        "servers": [ {
            "host": "127.0.0.1",
            "port": 2181
        } ],
        "timeout": 6000
    },
    "healthCheck": {
        "command": "date",
        "interval": 3000,
        "period": 15000,
        "threshold": 2
    }
}
